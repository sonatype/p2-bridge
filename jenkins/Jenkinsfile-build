library('private-pipeline-library')
library('jenkins-shared')

mavenSnapshotPipeline(javaVersion: 'Java 6', mavenVersion: 'Maven 3.0.x',
    onSuccess: { build, env ->
      notifyChat(env: env, currentBuild: build, room: 'nxrm-jenkins')
    },
    onFailure: { build, env ->
      notifyChat(env: env, currentBuild: build, room: 'nxrm-jenkins')
    }
)
